# vim: ft=conf
alias usbmount='mount /media/usb1'
alias uumount='umount /media/usb1/'
alias apnmount='mount /media/apn'
alias plantesmount='mount /media/plantes'
alias apnumount='umount /media/apn/'
alias phonemount='jmtpfs /media/phone'
alias apntmount='mount /media/apnthom'
alias apntumount='umount /media/apnthom/'
alias zicmount='mount /media/zic'
alias zicumount='umount /media/zic'
alias cammount='mount /media/cam'
alias clef='cd /media/usb1'
alias prez='cd /media/usb1/prez'
alias phone='cd /media/phone'
alias ls='ls --color=auto'
alias la='ls -ah'
alias ll='ls -lh'
alias lla='ls -alh'
alias ss='cd ~/scripts/ && ls'
alias zt='zathura'
alias rr='ranger'
# alias st='$(st -e tmux)'
# alias st='st -f "Inconsolata for Powerline"'
alias du1='du --max-depth=1 -h |sort -hr'
alias bt='bluetoothctl'
alias meteo='curl wttr.in/Marseille'
alias edore='cd /media/usb1/insee/edore/'
alias fst='cd /media/usb1/insee/edore/fst/'
alias w3m='w3m -M'
